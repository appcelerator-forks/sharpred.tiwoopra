<ti:app
  xmlns:ti="http://ti.appcelerator.org">
  <name>appname</name>
  <id>com.tonylukasavage.appname</id>
  <version>1.1</version>
  <property name="ti.ui.defaultunit" type="string">dp</property>
  <modules>
    <module platform="ios" version="1.0">com.appc.foo</module>
    <module platform="android">com.appc.foobar</module>
    <module platform="ios">com.appc.foobar</module>
    <module>com.appc.quux</module>
  </modules>
  <deployment-targets>
    <target device="android">true</target>
    <target device="ipad">true</target>
    <target device="iphone">true</target>
    <target device="mobileweb">false</target>
    <target device="tizen">false</target>
    <target device="blackberry">false</target>
  </deployment-targets>
  <plugins>
    <plugin version="2.0">ti.alloy</plugin>
    <plugin>no.version</plugin>
    <plugin version="3.3">another.plugin</plugin>
  </plugins>
  <property name="some.property" type="int">123</property>
  <sdk-version>3.2.2.GA</sdk-version>
</ti:app>